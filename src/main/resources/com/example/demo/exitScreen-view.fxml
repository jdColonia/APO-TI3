<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane prefHeight="450.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17.0.2-ea"
            xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.example.demo.ExitScreenController">
    <children>
        <ImageView fitHeight="450.0" fitWidth="800.0" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@exitScreen.png"/>
            </image>
        </ImageView>
        <Button fx:id="resetBtn" layoutX="183.0" layoutY="329.0" mnemonicParsing="false" onAction="#resetGame"
                prefHeight="70.0" prefWidth="190.0" style="-fx-background-color: TRANSPARENT;"/>
        <Button fx:id="exitBtn" layoutX="434.0" layoutY="329.0" mnemonicParsing="false" onAction="#exitGame"
                prefHeight="70.0" prefWidth="190.0" style="-fx-background-color: TRANSPARENT;"/>
    </children>
</AnchorPane>
